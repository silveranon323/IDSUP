from collections import Counter
c= Counter("gallad")
print(c)
c=Counter(['a','p','p','l','e'])
print(c)
c= Counter({'a':1,'b':2})
print(c)
c= Counter(cats=4,dogs=7)
print(c)
# using lambda function
word_counts = {'apple':3,'banana':5,'orange':2,'grape':4}
wc = sorted(word_counts.items(),key=lambda word_and_counts: word_and_counts[1] , reverse=True)
print(wc)
